{
    "version": 2,
    "name": "mini-page",
    "builds": [
        {
            "src": "static/**/*.*",
            "use": "@vercel/static"
        },
        {
          "src": "index.js",
          "use": "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src": "/static/(.*)",
            "headers": { "cache-control": "s-maxage=31536000,immutable" },
            "dest": "/static/$1"
        },
        {
            "src" : "/(.*)", "dest":"/index.js"
        }
    ],
    "env": {
        "SERVER_PORT": 8000
    }
}